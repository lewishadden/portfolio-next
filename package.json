{
  "name": "portfolio-next",
  "description": "Lewis Hadden online portfolio built with Next.js",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://portfolio.lewishadden.co.uk",
  "author": "Lewis Hadden <lewishadden@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:lewishadden/portfolio-next.git"
  },
  "engines": {
    "node": "20.9.0",
    "npm": "10.9.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-image-export-optimizer && npm run purgecss && next-sitemap",
    "purgecss": "purgecss -c purgecss.config.cjs",
    "analyze": "ANALYZE=true next build",
    "start": "serve out",
    "typecheck": "tsc --noEmit",
    "lint": "next lint && npm run lint:stylelint",
    "lint:fix": "next lint --fix && npm run lint:stylelint:fix",
    "lint:stylelint": "stylelint '**/*.{scss,css}' --cache",
    "lint:stylelint:fix": "stylelint '**/*.{scss,css}' --cache --fix",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,json,css,yml,md}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,js,json,css,scss,yml,md}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@mantine/core": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@tabler/icons-react": "^3.12.0",
    "bootstrap": "^5.3.3",
    "formik": "^2.4.6",
    "next": "^14.2.18",
    "react": "18.2.0",
    "react-awesome-slider": "^4.1.0",
    "react-bootstrap": "^2.10.1",
    "react-dom": "18.2.0",
    "react-headroom": "^3.2.1",
    "react-type-animation": "^3.2.0",
    "react-vertical-timeline-component": "^3.6.0",
    "sass": "^1.81.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@next/bundle-analyzer": "^14.1.3",
    "@next/eslint-plugin-next": "^14.1.3",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.1.3",
    "@storybook/nextjs": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "@types/react": "18.2.65",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-headroom": "^3.2.3",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-loader": "^9.1.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-mantine": "3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-image-export-optimizer": "^1.14.1",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "purgecss": "^6.0.0",
    "serve": "^14.2.3",
    "storybook": "^8.1.3",
    "storybook-dark-mode": "^4.0.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.2",
    "webpack": "^5.91.0"
  }
}
